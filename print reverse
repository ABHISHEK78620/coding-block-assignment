import java.util.*;
public class Main {
    public static void main(String args[]) {

        // Your Code Here
        Scanner sc = new Scanner(System.in);
        int n =sc.nextInt();
        int c=0;
        int q=0;
        while(n>0){
            c=n%10;
            q=10*q+c;
            n=n/10;
        }
        System.out.println(q);
    }
    
}

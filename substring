public class substring {
    public static void main(String gas[]){
        String str = "abcd";
        sub(str);

    }
    public static void sub(String str){
        
        for(int i =0;i<str.length();i++){
            for(int j = i+1;j<=str.length();j++){
                System.out.println(str.substring(i,j));
            }
        }
    }
}
